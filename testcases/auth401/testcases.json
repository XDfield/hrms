  {
    "name": "访问需要鉴权的API接口-无效cookie",
    "method": "POST",
    "url": "/authority/create",
    "headers": {
      "Content-Type": "application/json",
      "Cookie": "user_cookie=invalid_cookie_format"
    },
    "body": {
      "user_type": "normal",
      "model": "staff_manage",
      "name": "员工管理模块",
      "authority_content": "查询、添加、编辑"
    },
    "expectedStatus": 401,
    "expectedBody": {
      "status": 401,
      "message": "Unauthorized"
    },
    "description": "使用无效cookie访问需要鉴权的API接口应返回401状态码",
    "category": "auth401",
    "enabled": true
  },
