[
  {
    "name": "职级创建接口-无cookie鉴权测试",
    "method": "POST",
    "url": "/rank/create",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "rank_name": "测试职级_{{datetime}}"
    },
