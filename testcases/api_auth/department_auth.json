[
  {
    "name": "部门创建接口-无cookie鉴权测试",
    "method": "POST",
    "url": "/depart/create",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "dep_name": "测试部门_{{datetime}}",
      "dep_describe": "测试部门描述"
