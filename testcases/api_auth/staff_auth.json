[
  {
    "name": "员工创建接口-无cookie鉴权测试",
    "method": "POST",
    "url": "/staff/create",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "staff_name": "测试员工_{{datetime}}",
      "leader_staff_id": "admin",
      "phone": 13800138000,
      "birthday_str": "1990-01-01",
      "identity_num": "123456199001011237",
      "dep_id": "dep_001",
      "rank_id": "rank_001",
      "entry_date": "2024-01-01",
      "user_password": "123456"
    },
    "expectedStatus": 401,
    "expectedBody": {
      "status": 401,
      "message": "Unauthorized"
    },
    "description": "无cookie访问员工创建接口应返回401状态码",
    "category": "api_authentication",
    "enabled": true
  },
  {
    "name": "员工编辑接口-无效cookie鉴权测试",
    "method": "POST",
    "url": "/staff/edit",
    "headers": {
      "Content-Type": "application/json",
      "Cookie": "user_cookie=malformed_cookie"
    },
    "body": {
      "staff_id": "staff_test_001",
      "staff_name": "编辑测试员工",
      "phone": 13900139000
    },
    "expectedStatus": 401,
    "expectedBody": {
      "status": 401,
      "message": "Unauthorized"
    },
    "description": "无效cookie访问员工编辑接口应返回401状态码",
    "category": "api_authentication",
    "enabled": true
  },
  {
    "name": "员工查询接口-无cookie鉴权测试",
    "method": "GET",
    "url": "/staff/query",
    "headers": {},
    "expectedStatus": 401,
    "expectedBody": {
      "status": 401,
      "message": "Unauthorized"
    },
    "description": "无cookie访问员工查询接口应返回401状态码",
    "category": "api_authentication",
    "enabled": true
  },
  {
    "name": "密码查询接口-无cookie鉴权测试",
